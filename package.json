    "scripts": {
        "swagger": "swagger-cli bundle ./swagger/swagger.yaml --outfile swagger.yaml --type yaml",
        "swagger-json": "swagger-cli bundle ./swagger/swagger.json --outfile swagger.json --type json",
        "start": "nodemon src/index.ts",
        "staging": "cross-env NODE_OPTIONS=--max-old-space-size=12192 NODE_ENV=staging  ts-node src/index.ts",
        "staging-info": "cross-env NODE_OPTIONS=--max-old-space-size=12192 NODE_ENV=staging-info  ts-node src/index.ts",
        "production": "cross-env NODE_ENV=production ts-node src/index.ts",
        "staging-stop": "sudo systemctl stop icons",
        "staging-sync": "cross-env NODE_ENV=staging ts-node scripts/sync.ts",
        "staging-start": "sudo systemctl start icons",
        "staging-status": "sudo systemctl status icons",
        "staging-info-stop": "sudo systemctl stop icons",
        "staging-info-sync": "cross-env NODE_ENV=staging ts-node scripts/sync.ts",
        "staging-info-start": "sudo systemctl start icons",
        "staging-info-status": "sudo systemctl status icons",
        "staging-log": "sudo journalctl -u icons -n 50 -f",
        "prettier-format": "prettier --config .prettierrc src/**/*.ts --write"
    }
